{
  "extends": "airbnb",
  "rules": {
    "no-underscore-dangle": 0,
    "no-restricted-syntax": 0,
    "class-methods-use-this": 0,
    "import/no-unresolved": 0,
    "function-paren-newline": 0,
    "import/extensions": 0,
    "no-prototype-builtins": 0,
    "no-console": ["error", { "allow": ["warn", "error"] }]
  },
  "globals": {
     "global": true,
     "document": true,
     "window": true,
     "expect": true,
     "describe": true,
     "it": true,
     "after": true,
     "before": true,
     "beforeEach": true,
     "afterEach": true,
     "sinon": true
  }
}
